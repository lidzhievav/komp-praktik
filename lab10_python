import math

# Константы
G = 6.672e-11  # Гравитационная постоянная
M = 5.96e24    # Масса Земли
R = 6.37e6     # Радиус Земли

# Ввод данных
h = float(input("Введите высоту над поверхностью Земли (в метрах): "))

# Расчет ускорения свободного падения
g = G * M / (R + h) ** 2

# Вывод результата
print(f"{g:.4f} м/с^2")  # Устанавливаем точность вывода
